<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="polish_form_update_text" model="ir.ui.view">
            <field name="name">polish.update.text.wizard.form</field>
            <field name="model">polish.update.text.wizard</field>
            <field name="arch" type="xml">
                <form string="Create New Polish">
                    <group>
                    <group>
                        <field name="note"/>
                        </group>
                    </group>
                    <footer>
                        <button name="update_polish_text_wizard" type="object" string="Update note"/>
                        <button name="action_open_contacts" type="object" string="Open Contacts"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</data>
</odoo>



